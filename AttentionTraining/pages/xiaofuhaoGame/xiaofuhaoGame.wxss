/* pages/xiaofuhaoGame/xiaofuhaoGame.wxss */
.container {
  padding: 0rpx;
  margin: 0rpx;
  width: 100%;
  height: 100%;
}

.timeCircle{
  background-color: white;
  width:100%;
  margin-top: 10rpx;
  height:80px;
  justify-content:center;
  position: relative;
}
.bgCanvas{
  display: flex;
  width:100%;
  height:80px;
  margin: 0 auto;
  position: absolute;
}
.stepText{
  display: flex;
  font-weight: bold;
  font-size: 28px;
  width:100%;
  height: 80px;
  color: green;
  align-items: center;
  justify-content:center;
}

.main-view {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.title-view {
  margin-top: 26rpx;
  width: 750rpx;
  height: 54rpx;
  margin-left: 0rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.title-text {
  margin-left: 28rpx;
}

.scroll_box{
  width: 100%;
  overflow: hidden;
  background-color: #fff;
  white-space: nowrap;
  /* white-space: normal; */
}
.gameView {
  width: 100%;
  height: 500rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

/* 工作记忆区训练游戏 */
.xiaofuhao {
  position: fixed;
  width: 100%;
  height: 500rpx;
  /* background-color: lightgray; */
}

.table{
  position: flex;
  border-left: 1px solid black;
  border-top: 1px solid black;
  margin-top: 30rpx;
  width: 80%;
  margin-left: 10%;
  height: 200rpx;
}

.tr{
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.th,.td{
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid black;
  border-right: 1px solid black;
  text-align: center;
  width: 100%;
  font-weight: bold;
  font-size: 40rpx;
  color: black;
}

/* 结束图 */
.shadow-result {
  position: fixed;
  flex-direction: row;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.75);
}

.start-view-result {
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 942rpx;
  width: 100%;
}

.tankuang2-result {
  position: absolute;
  width: 660rpx;
  height: 942rpx;
  display: block;
  z-index: -999;
}

.describle-view {
  margin-top: 40%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  height: 942rpx;
  width: 100%;
}

.describle-view-item {
  width: 70%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.info-1, .info-3 {
  height: 50rpx;
  color: gray;
  font-size: 35rpx;
  font-weight: normal;
}

.info-2 {
  color: black;
  font-size: 45rpx;
  font-weight: bold;
}

.info-end {
  height: 50rpx;
  width: 70%;
  color: chocolate;
  font-size: 38rpx;
  font-weight: bold;
  display: block;
}

.info-buttons {
  margin-top: 50rpx;
  width: 80%;
  height: 0rpx;
  display: flex;
  flex-direction: row;
  /* justify-content: space-between; */
  text-align: center;
}

.info-once-more, .info-share-report {
  width: 280rpx;
  height: 90rpx;
  background-color: rgba(1,1,1,0);
}
.info-once-more::after {
  border: 0;
}
.info-share-report::after {
  border: 0;
}

.info-once-more image {
  width: 240rpx;
  height: 90rpx;
}
.info-share-report image {
  width: 240rpx;
  height: 90rpx;
}
.info-cancel {
  margin-top: 150rpx;
  width: 60%;
  height: 100rpx;
}

.result-view {
  height: 80rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: -40rpx;
  /* background-color: greenyellow; */
}

.tick-image {
  height: 52rpx;
  width: 52rpx;
}

.cross-image {
  height: 52rpx;
  width: 52rpx;
}

.bottom-view {
  position: fixed;
  width: 100%;
  height: 140rpx;
  bottom: 0rpx;
  background-color: #923179;
  display: flex;
  flex-direction: row;
  z-index: 103;
}

.different-button {
  margin-top: 0rpx;
  height: 100%;
  width: 373rpx;
  margin-left: 0rpx;
  background-color: #923179;
  padding: 0rpx;
  font-size:60rpx;
  font-weight:bold;
  color:white;
  line-height:140rpx;
}

.line-view {
  height: 70rpx;
  width: 4rpx;
  margin-top: 35rpx;
  background-color: #fff;
}

.identical-button {
  margin-top: 0rpx;
  height: 100%;
  width: 373rpx;
  margin-right: 0rpx;
  background-color: #923179;
  padding: 0rpx;
  font-size:60rpx;
  font-weight:bold;
  color:white;
  line-height:140rpx;
}

.keyboard {
  position: absolute;
  width: 100%;
  height: 365rpx;
  bottom: 0px;
  background-color: #D5DADD;
  border-radius: 25rpx 25rpx 0 0;
}

.keyboard-row {
  margin-top: 10rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

button{
  background-color: #fff; 
  width: 100rpx;
  height: 85%;
}

/*底部键盘样式*/
.footer {
  position: fixed;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #D5DADD;
  width: 750rpx;
  height: 400rpx;
  bottom: 0rpx;
}

/* 第一行 */
.lineOne {
  margin-top: 20rpx;
  width: 750rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.btnOne {
  width:148rpx;
  height:110rpx;
  background-color: #fff;
  border-radius:10rpx;
  padding: 0rpx;
  font-size:65rpx;
  font-weight:600;
  color:black;
  line-height:110rpx;
}

/* 第一行 */
.lineTwo {
  margin-top: 20rpx;
  width: 780rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.btnTwo {
  width:190rpx;
  height:110rpx;
  background-color: #fff;
  border-radius:10rpx;
  padding: 0rpx;
  font-size:65rpx;
  font-weight:600;
  color:black;
  line-height:110rpx;
}

.info-shadow {
  position: fixed;
  flex-direction: row;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.75);
}
.info-shadow-img {
  width: 100%;
  height: 100%;
  text-align: center;
}
.info-shadow-pic {
  width: 662rpx;
  height: 1188rpx;
  z-index: -1;
}

.title-image {
  position: absolute;
  width: 52rpx;
  height: 52rpx;
  margin-top: 0rpx;
  margin-left: 670rpx;
}