  width: 100%;
  height: 100%;
}

/* 头部倒计时 */
.main-view {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 40rpx;
}

.time-view {
  /* margin-top: 32rpx; */
  width: 130rpx;
  height: 130rpx;
}

.time-view image {
  position: absolute;
  width: 130rpx;
  height: 130rpx;
}

.time-view .timer {
  position: relative;
  height: 130rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.time-view .timer text {
  height:130rpx;
  width: 130rpx;
  font-size:78rpx;
  font-weight:bold;
  color: #333333;
  line-height:130rpx;
  text-align: center;
}

.runCanvas {
  position: absolute;
  width: 130rpx;
  height: 130rpx;
  transform: translate3d(-999rpx, 0, 0);

}

.scroll_box{
  margin-top: 30rpx;
  width: 100%;
  overflow: hidden;
  background-color: #fff;
  white-space: nowrap;
  /* white-space: normal; */
}

.scroll_box scroll-view{
  /* height: 100%;
